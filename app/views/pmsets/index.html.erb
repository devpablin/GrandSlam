<p id="notice"><%= notice %></p>

<h1>Sets</h1>

<table>
  <thead>
    <tr>
      <th>Local points</th>
      <th>Visitor points</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pmsets.each do |pmset| %>
      <tr>
        <td><%= pmset.local_points %></td>
        <td><%= pmset.visitor_points %></td>
        <td><%= link_to 'Show', pmset , class: "btn btn-info"%></td>
        <td><%= link_to 'Edit', edit_pmset_path(pmset) , class: "btn btn-warning"%></td>
        <td><%= link_to 'Destroy', pmset, method: :delete, data: { confirm: 'Are you sure?' } , class: "btn btn-danger"%></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pmset', new_pmset_path , class: "btn btn-success"%>
