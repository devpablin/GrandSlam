<p id="notice"><%= notice %></p>

<h1>Pmcountries</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pmcountries.each do |pmcountry| %>
      <tr>
        <td><%= pmcountry.name %></td>
        <td><%= link_to 'Show', pmcountry %></td>
        <td><%= link_to 'Edit', edit_pmcountry_path(pmcountry) %></td>
        <td><%= link_to 'Destroy', pmcountry, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pmcountry', new_pmcountry_path %>
