<p id="notice"><%= notice %></p>

<h1>Pmrounds</h1>

<table>
  <thead>
    <tr>
      <th>Round</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pmrounds.each do |pmround| %>
      <tr>
        <td><%= pmround.round %></td>
        <td><%= link_to 'Show', pmround %></td>
        <td><%= link_to 'Edit', edit_pmround_path(pmround) %></td>
        <td><%= link_to 'Destroy', pmround, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pmround', new_pmround_path %>
