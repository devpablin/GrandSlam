<p id="notice"><%= notice %></p>

<h1>Pmtrainers</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Apellido</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pmtrainers.each do |pmtrainer| %>
      <tr>
        <td><%= pmtrainer.nombre %></td>
        <td><%= pmtrainer.apellido %></td>
        <td><%= link_to 'Show', pmtrainer %></td>
        <td><%= link_to 'Edit', edit_pmtrainer_path(pmtrainer) %></td>
        <td><%= link_to 'Destroy', pmtrainer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pmtrainer', new_pmtrainer_path %>
