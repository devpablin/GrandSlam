<p id="notice"><%= notice %></p>

<h1>Pmplayers</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Fechanacimiento</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pmplayers.each do |pmplayer| %>
      <tr>
        <td><%= pmplayer.first_name %></td>
        <td><%= pmplayer.last_name %></td>
        <td><%= pmplayer.fechanacimiento %></td>
        <td><%= link_to 'Show', pmplayer %></td>
        <td><%= link_to 'Edit', edit_pmplayer_path(pmplayer) %></td>
        <td><%= link_to 'Destroy', pmplayer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pmplayer', new_pmplayer_path %>
