<p id="notice"><%= notice %></p>

<h1>Pmnationalities</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pmnationalities.each do |pmnationality| %>
      <tr>
        <td><%= pmnationality.name %></td>
        <td><%= link_to 'Show', pmnationality %></td>
        <td><%= link_to 'Edit', edit_pmnationality_path(pmnationality) %></td>
        <td><%= link_to 'Destroy', pmnationality, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pmnationality', new_pmnationality_path %>
